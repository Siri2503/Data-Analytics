

lst =[[1,2,3],
[4,5,6],
[7,8,9]]


rows = len(lst)
isSquare =True

for i in range(len(lst)):
    if len(lst[i]) != rows:
        isSquare = False
        break
if isSquare:
    print("Square matrix")
else:
    print("not square matrix")
lst_even =[]       
for i in range(len(lst)):
    lst_ = []
    for j in range(len(lst[i])):
        if lst[i][j] %2==0:
            lst_.append(lst[i][j])
    lst_even.append(lst_)
print(lst_even)



